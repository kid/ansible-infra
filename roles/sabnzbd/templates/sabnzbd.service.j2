[Unit]
Description=SABnzbd binary newsreader
Documentation=https://sabnzbd.org/wiki/
Wants=network-online.target
After=network-online.target

[Service]
Environment="PYTHONIOENCODING=utf-8"
ExecStart=/opt/sabnzbd/SABnzbd.py --disable-file-log --logging 1 --browser 0 --server 0.0.0.0:8080 --config-file /var/lib/sabnzbd/sabnzbd.ini
User=sabnzbd
Group=sabnzbd
Type=simple
Restart=on-failure

[Install]
WantedBy=multi-user.target
